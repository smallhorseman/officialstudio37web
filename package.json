{
  "name": "officialstudio37web",
  "version": "1.0.0",
  "type": "module",
  "description": "Studio37 Photography Website - Professional photography and content strategy services in Houston, TX",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "build:analyze": "ANALYZE=true vite build",
    "preview": "vite preview --host",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules/.vite",
    "audit:security": "npm audit",
    "audit:performance": "npm run build && npx lighthouse http://localhost:4173 --output json --output-path ./lighthouse-report.json"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "react-markdown": "^8.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "postcss": "^8.4.31",
    "rollup-plugin-visualizer": "^5.9.2",
    "tailwindcss": "^3.3.5",
    "terser": "^5.24.0",
    "typescript": "^5.0.0",
    "vite": "^4.5.0"
  },
  "keywords": [
    "photography",
    "houston",
    "content-strategy",
    "professional-photographer",
    "studio37",
    "react",
    "supabase",
    "photography-portfolio"
  ],
  "author": "Studio37",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/smallhorseman/officialstudio37web.git"
  },
  "homepage": "https://www.studio37.cc",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
